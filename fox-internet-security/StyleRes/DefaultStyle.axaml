<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Design.PreviewWith>
    <Border Padding="20">
      <!-- Add Controls for Previewer Here -->
    </Border>
  </Design.PreviewWith>

  <!-- Add Styles Here -->
	<Style Selector="Window">

	</Style>
	<Style Selector="Button.sideMenu">
		<Setter Property="FontSize" Value="20"/>
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="FontWeight" Value="Normal"/>
	</Style>
	<Style Selector="Button">
		<Setter Property="FontSize" Value="20"/>
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="FontWeight" Value="Normal"/>
	</Style>
	<Style Selector="Label.icon">
		<Setter Property="FontFamily" Value="{DynamicResource ResourceKey=MaterialIcons}"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="FontSize" Value="20"/>
		<Setter Property="FontWeight" Value="Heavy"/>
		<Setter Property="Margin" Value="35,0,0,0"/>
	</Style>
	<Style Selector="Label.akkoTimeLineHead">
		<Setter Property="FontFamily" Value="{DynamicResource ResourceKey=MaterialIcons}"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="FontSize" Value="20"/>
	</Style>
	<Style Selector="Label.akkoTimeLineText">
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="FontSize" Value="18"/>
	</Style>
	<Style Selector="Label.iconTimeLineHead">
		<Setter Property="FontFamily" Value="{DynamicResource ResourceKey=MaterialIcons}"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="FontSize" Value="20"/>
		<Setter Property="FontWeight" Value="Heavy"/>
	</Style>
	<Style Selector="Label.iconTimeLineText">
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="FontSize" Value="18"/>
		<Setter Property="FontWeight" Value="Heavy"/>
	</Style>
</Styles>
