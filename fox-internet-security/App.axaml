<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="fox_internet_security.App"
             xmlns:local="using:fox_internet_security"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="/StyleRes/DefaultStyle.axaml"></StyleInclude>
    </Application.Styles>

	<Application.Resources>
		<FontFamily x:Key="MaterialIcons">/Assets/Fonts/MaterialSymbolsRounded.ttf#Material Symbols Rounded</FontFamily>
		
	</Application.Resources>

</Application>